# Variables de entorno:
SHELL=/bin/bash
HOME=/home/ciencia.datos

# Cron jobs:
 0 0 * * mon-sat echo "$(date), docker stop, $(docker stop $(docker ps --quiet))"               >> ${HOME}/log/cron.log
10 0 * * mon-sat echo "$(date), run_docker, $(${HOME}/repositorios/servidor/src/run_docker.sh)" >> ${HOME}/log/cron.log

